    <Member MemberName="ShowLights">
      <MemberSignature Language="C#" Value="ShowLights" />
      <MemberSignature Language="ILAsm" Value=".field public static literal valuetype Android.App.NotificationFlags ShowLights = int32(1)" />
      <MemberSignature Language="DocId" Value="F:Android.App.NotificationFlags.ShowLights" />
