    <Member MemberName="DSTSavings">
      <MemberSignature Language="C#" Value="public virtual int DSTSavings { get; }" />
      <MemberSignature Language="ILAsm" Value=".property instance int32 DSTSavings" />
      <MemberType>Property</MemberType>
      <AssemblyInfo>
