          <AttributeName Language="F#">[&lt;Android.Runtime.Register("eglDupNativeFenceFDANDROID", "(Landroid/opengl/EGLDisplay;Landroid/opengl/EGLSync;)Landroid/hardware/SyncFence;", "", ApiSince=33)&gt;]</AttributeName>
