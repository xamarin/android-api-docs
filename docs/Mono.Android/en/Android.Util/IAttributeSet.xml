<Type Name="IAttributeSet" FullName="Android.Util.IAttributeSet">
  <TypeSignature Language="C#" Value="public interface IAttributeSet : Android.Runtime.IJavaObject, IDisposable" />
  <TypeSignature Language="ILAsm" Value=".class public interface auto ansi abstract IAttributeSet implements class Android.Runtime.IJavaObject, class System.IDisposable" />
