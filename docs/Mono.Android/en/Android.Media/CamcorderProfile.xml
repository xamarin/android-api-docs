    <Member MemberName="Get">
      <MemberSignature Language="C#" Value="public static Android.Media.CamcorderProfile Get (int cameraId, Android.Media.CamcorderQuality quality);" />
      <MemberSignature Language="ILAsm" Value=".method public static hidebysig class Android.Media.CamcorderProfile Get(int32 cameraId, valuetype Android.Media.CamcorderQuality quality) cil managed" />
      <MemberType>Method</MemberType>
